{
  "name": "online-movie-catalog",
  "version": "1.2.6",
  "description": "Standalone Online Movie Catalog App",
  "main": "main.js",
  "productName": "Online Movie Catalog",
  "homepage": "https://example.com/onlinemoviecatalog",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "build:linux": "electron-builder --linux deb"
  },
  "author": {
    "name": "kaptanoguz",
    "email": "kaptanoguz@gmail.com"
  },
  "license": "GPL-3.0",
  "dependencies": {},
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.oguz.onlinemoviecatalog",
    "productName": "Online Movie Catalog",
    "directories": {
      "output": "dist"
    },
    "linux": {
      "target": [
        "deb"
      ],
      "maintainer": "kaptanoguz",
      "category": "Video;AudioVideo;"
    },
    "deb": {
      "priority": "optional"
    },
    "files": [
      "main.js",
      "src/**/*",
      "node_modules/**/*"
    ]
  }
}